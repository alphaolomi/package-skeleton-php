{
    "name": "alphaolomi/simplify-vfd",
    "description": "Simplify VFD",
    "keywords": [
        "alphaolomi",
        "simplify-vfd",
        "simplify-vfd"
    ],
    "homepage": "https://github.com/alphaolomi/simplify-vfd",
    "license": "MIT",
    "authors": [
        {
            "name": "Alpha Olomi",
            "email": "hello@alphaolomi.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^2.20",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Alphaolomi\\SimplifyVfd\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alphaolomi\\SimplifyVfd\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
